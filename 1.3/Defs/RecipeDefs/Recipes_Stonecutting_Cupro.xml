<?xml version="1.0" encoding="utf-8" ?>
<Defs>

    <RecipeDef ParentName="MakeStoneBlocksBulkBase" MayRequire="Ogliss.Cupro.CuprosStones">
        <defName>Make_StoneBlocksBulkClaystone</defName>
        <label>make claystone blocks x3</label>
        <description>Cuts claystone chunks into usable blocks in bulk.</description>
        <ingredients>
            <li>
                <filter>
                    <thingDefs>
                        <li>ChunkClaystone</li>
                    </thingDefs>
                </filter>
                <count>3</count>
            </li>
        </ingredients>
        <fixedIngredientFilter>
            <thingDefs>
                <li>ChunkClaystone</li>
            </thingDefs>
        </fixedIngredientFilter>
        <products>
            <BlocksClaystone>60</BlocksClaystone>
        </products>
    </RecipeDef>

