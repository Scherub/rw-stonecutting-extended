    <RecipeDef ParentName="MakeStoneBlocksBulkBase" MayRequire="Kura.ExtraStoneCupro">
        <defName>Make_StoneBlocksBulkDarkAndesiteESC</defName>
        <label>make dark andesite blocks x3</label>
        <description>Cuts dark andesite chunks into usable blocks in bulk.</description>
        <ingredients>
            <li>
                <filter>
                    <thingDefs>
                        <li>ChunkDarkAndesite</li>
                    </thingDefs>
                </filter>
                <count>3</count>
            </li>
        </ingredients>
        <fixedIngredientFilter>
            <thingDefs>
                <li>ChunkDarkAndesite</li>
            </thingDefs>
        </fixedIngredientFilter>
        <products>
            <BlocksDarkAndesite>60</BlocksDarkAndesite>
        </products>
    </RecipeDef>

