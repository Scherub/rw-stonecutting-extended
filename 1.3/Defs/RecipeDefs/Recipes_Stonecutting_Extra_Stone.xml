<Defs>

    <RecipeDef ParentName="MakeStoneBlocksBulkBase" MayRequire="Kura.ExtraStone">
        <defName>Make_StoneBlocksBulkAndesite</defName>
        <label>make andesite blocks x3</label>
        <description>Cuts andesite chunks into usable blocks in bulk.</description>
        <ingredients>
            <li>
                <filter>
                    <thingDefs>
                        <li>ChunkAndesite</li>
                    </thingDefs>
                </filter>
                <count>3</count>
            </li>
        </ingredients>
        <fixedIngredientFilter>
            <thingDefs>
                <li>ChunkAndesite</li>
            </thingDefs>
        </fixedIngredientFilter>
        <products>
            <BlocksAndesite>60</BlocksAndesite>
        </products>
    </RecipeDef>

