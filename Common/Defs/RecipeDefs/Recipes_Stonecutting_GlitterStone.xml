<?xml version="1.0" encoding="utf-8" ?>
<Defs>

    <RecipeDef ParentName="MakeStoneBlocksBulkBase" MayRequire="Harmonea.GlitterStone">
        <defName>Make_StoneBlocksHephalithGS</defName>
        <label>make hephalith blocks x3</label>
        <description>Cuts hephalith chunks into usable blocks in bulk.</description>
        <ingredients>
            <li>
                <filter>
                    <thingDefs>
                        <li>ChunkHS_Hephalith</li>
                    </thingDefs>
                </filter>
                <count>3</count>
            </li>
        </ingredients>
        <fixedIngredientFilter>
            <thingDefs>
                <li>ChunkHS_Hephalith</li>
            </thingDefs>
        </fixedIngredientFilter>
        <products>
            <BlocksHS_Hephalith>60</BlocksHS_Hephalith>
        </products>
    </RecipeDef>

