<Defs>

    <RecipeDef ParentName="MakeStoneBlocksBulkBase" MayRequire="Kura.ExtraStone">
        <defName>Make_StoneBlocksBulkAndesiteES</defName>
        <label>make andesite blocks x3</label>
        <description>Cuts andesite chunks into usable blocks in bulk.</description>
        <ingredients>
            <li>
                <filter>
                    <thingDefs>
                        <li>ChunkKura_Andesite</li>
                    </thingDefs>
                </filter>
                <count>3</count>
            </li>
        </ingredients>
        <fixedIngredientFilter>
            <thingDefs>
                <li>ChunkKura_Andesite</li>
            </thingDefs>
        </fixedIngredientFilter>
        <products>
            <BlocksKura_Andesite>60</BlocksKura_Andesite>
        </products>
    </RecipeDef>

